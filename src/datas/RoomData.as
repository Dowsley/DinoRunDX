package datas {		public class RoomData {				public var r:Object			//main data object		private var brain:Object	//ref to brain				public function RoomData (brain) {						this.brain=brain;			resetData();					}				public function resetData () {						for (var w in r) {				for  (var rm in r[w] ) {					for (var o in r[w][rm]) {						for (var i in r[w][rm][o]) {							delete r[w][rm][o][i]						}						delete r[w][rm][o] 					}					delete r[w][rm]				}				delete r[w]			}			r=null;			r = {};					}				public function adjustXY () {						//adjust x and y			for each (var world:Object in r) {								for (var roomID in world) {										// find x and y adjusters					var tmpA=roomID.split("_");					var xa=(tmpA[0].substring(1,tmpA[0].length)-1)*brain.sDim.w;					var ya=(tmpA[1]-1)*brain.sDim.h;										for each (var item:Object in world[roomID]) {												item.px+=xa;						item.py+=ya;											}									}							}					}	}	}