package supers {		import flash.geom.Point;	import flash.display.MovieClip;	import gfx.DinoClip;		public class FX extends DinoClip {		public var o:Object;   	   	 		// data object		public var brain:Object; 			// ref to brain		public var master:MovieClip;		// ref to master		public var ends:Boolean=false;		public var NN:Boolean=false;		//near near var?				//////////////////////////////////////////////////		//////////////////////////////////////////////////				// constructor		public function init (o,br,ms) {						this.o=o;			x= o.px;			y= o.py;			brain = br;			master=ms.graphic;					}				public function mainFX () {						if (master.ends) { deleteAll() }		}		   	    public function deleteAll():void {									//brain.deleteClip(this);			brain.deleteObj(this);					}				public function nulls () {						if (!o.rData&&!o.addToRD)  { for (var oo in o) { delete o[oo] } }						brain=null;			master=null;			o=null;					}				//final actions		public function finalActions():void {											}				// give a rounded random number		public function rand(min:int, max:int):int {						return (Math.round(Math.random()*(max-min))+min);					}		// give an unrounded random number		public function rand2(min:Number, max:Number):Number {						return (Math.random()*(max-min))+min;					}			}	}