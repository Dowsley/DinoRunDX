package gfx {		import flash.display.MovieClip;		import supers.DinoNP;		public class Cera extends DinoNP {				public var extraA:Object;		private var xh:Array;		public var pData:Object;				override public function nulls() {						super.nulls();			xh=null;			pData=null;					}				public function Cera (o,br) {						//dino init			//dino init			var v:int = Math2.randInt(1,3);			if (br.S.gh) {				v=1;			}			switch (v) {				case 1:				case 2:					assignGFX(new _Cera);					break;				case 3:					assignGFX(new _Cera2);					break;			}			super.init (o,br,this);						//extra physics properties			ph.f = .9;			//friction			ph.bn = 0;			//bounce			ph.g = .5;			//gravity			ph.m = 250;			//mass			ph.r = 45;			//radius						//other vars			var am:Number;			switch (brain.S.difNum) {								case 1: am=.4; break;				case 2: am=.5; break;				case 3: am=.6; break;				case 4: am=.8; break;							}			accelMax=accelMaxBase=rand2(am-.2,am+.2);			accelF=accelFBase=.01;			angI=15;						//land area for raptor			hasExtra=true;			extraA = brain.create ( {nam:"CeraA", px:x, py:y+42, dad:this, r:72, depth:["world","main"]} )						//init dir			if (rand(0,1)==0) {								keys.R=true;				vel.x=1;							} else {								keys.L=true;				vel.x=-1;							}										//misc			xh=[0,0];			pData = { score:250 }						//color var			if (!brain.S.gh) {				var offR:int=0;				var offG:int=0;				var offB:int=0;				switch (brain.S.colorV) { 									case "B": offR=offB=rand(-10,-40); offG=-10; break ;									}				cc.cChange ([offR,offG,offB,1,1,1,1]);			}			//broadcast creation?			if (brain.S.mod=="MP"&&o.remote==undefined) { brain.sys.MP.sendToRoom ("OC"+"px:"+rnd(o.px)+";py:"+rnd(o.py)+";am:"+accelMax+";rdt:"+(o.rData?roomID:0)+";nam:Cera") }					}				// MAIN LOOP								public function main () {						//run functions common to all Dinos			mainDino ();						//rev?			xh.unshift (pos.x)			xh.pop();			if (Math.abs(xh[0]-xh[1])<.1) { rev() }								}				public function removeExtras() {						extraA.deleteAll();			extraA=null;			hasExtra=false;					}				}	}