package gfx {		import supers.MovingClip;	//		public class EggShell extends MovingClip {				private var c:uint=0;				public function EggShell (o,br):void {						//init			assignGFX(new _EggShell);			super.init (o,br,this);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .95,			//friction			ph.bn = 0,			//bounce			ph.g = .3,			//gravity			ph.m = 1			//mass						//appearance			graphic.gotoAndPlay ("f"+rand(1,4));						}				public function main ():void {						//run functions common to all MovingClips			mainMC ();						c++;			if (c>100) { deleteAll() }		}				// REACTIONS				public function bounce (v) {						stop();			vel.x=v.x;			vel.y=v.y;					}				public function remove (obj) {						deleteAll();					}			}	}