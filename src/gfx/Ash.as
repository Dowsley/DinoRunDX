package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Ash extends MovingDot {				private var c:int=0;		private var hit:Boolean=false;				public function Ash (o,brain):void {						var colors:Array = [0xD75200,0xD56A00,0xD58000,0xD54000]						//init			init (o, brain, false, colors[rand(0,colors.length-1)]);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .9,			//friction			ph.bn = 0,			//bounce			ph.g = .05,			//gravity			ph.m = 1			//mass			ph.r = 12;						inter=false;						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						//fall like a feather			vel.x*=.96;			vel.y*=.96;			vel.x+=(hit?0:rand2(-.5,.5));			vel.y+=rand2(-.05,.05);						c++;			if (c>300) { deleteAll() }			if (c==75) { ph.r=5; inter=true }		}				// REACTIONS				public function bounce (v) {						hit=true;			vel.x=v.x;			vel.y=v.y;					}				public function remove (obj) {						deleteAll();					}			}	}