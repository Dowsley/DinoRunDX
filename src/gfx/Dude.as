package gfx {		import flash.display.MovieClip;		import supers.Clip;		public class Dude extends Clip {				var c:int=0;		var col:Boolean=false;				public function Dude (o,brain):void {						assignGFX(new _Dude);			init (o,brain,this);						//extra physics properties			ph.sh = "circ";					}				public function main ():void {						//functions for all Clips			mainC();						if (c>0) {								c++;				if (c>75) { deleteAll() }							}					}				// REACTIONS				// collected		public function collected (obj) {						if (!col) {								col=true;				brain.sys.audio.ply ("misc1",.7,false);				brain.sys2.stats.dudeGame=true;				graphic.gotoAndPlay ("flip");				c=1;							}					}			}	}