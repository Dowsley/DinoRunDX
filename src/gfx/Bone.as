package gfx {		import supers.MovingClip;	//		public class Bone extends MovingClip {				private var c:uint=0;		private var fr:uint;				public function Bone (o,br):void {						//init			assignGFX(new _Bone);			super.init (o,br,this);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .1,			//friction			ph.bn = .2,			//bounce			ph.g = .5,			//gravity			ph.m = 1			//mass						//appearance			fr = rand(1,15);			graphic.gotoAndPlay ("f"+fr);						if (brain.S.gh) {				limit = 500;				ph.f = .97,			//friction				ph.bn = .8,			//bounce				ph.g = .4,			//gravity				ph.m = 10,				ph.r = 10;			}						}				public var bc:Boolean=false;				public function hit (obj:Object) {						if (!bc && brain.S.gh) {								bc=true;				brain.sys.audio.ply ("bone"+rand(1,4),.25,false);								if (brain.S.mod!="MP") {					var val:int = rand(1,5);					brain.sys2.stats.addBones (val);					brain.create ( {nam:"ScoreFloat", px:x, py:y, val:val, depth:["world","main"]} );				}				vel.x += rand(-200,200);				vel.y += rand(-100,0);				ph.r=15;				graphic.gotoAndPlay ("f"+fr);				}					}				private var limit:int = 130;		public function main ():void {						//run functions common to all MovingClips			mainMC ();						if (brain.S.gh && bc) {				graphic.alpha -= .02;			}						c++;			if (c>limit) { deleteAll() }		}				// REACTIONS				public function bounce (v) {						if (brain.S.gh && ph.r==15) {							} else {				graphic.gotoAndStop ("s"+fr);			}			vel.x=v.x;			vel.y=v.y;					}				public function remove (obj) {						deleteAll();					}			}	}