package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class EggParticle extends MovingDot {				private var c:uint=0;				public function EggParticle (o,brain):void {						//init			init (o, brain, false, 0xFFFFFF);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .97,			//friction			ph.bn = 0,			//bounce			ph.g = .3,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						c++;			if (c>100) { deleteAll() }		}				// REACTIONS				public function bounce (v) {			vel.x=v.x;			vel.y=v.y;					}				public function remove (obj) {						deleteAll();					}			}	}