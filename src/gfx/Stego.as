package gfx {		import flash.display.MovieClip;		import supers.DinoNP;		public class Stego extends DinoNP {				public var extraA:Object;		private var xh:Array;		public var pData:Object;				override public function nulls() {						super.nulls();			pData=null;			xh=null;					}				public function Stego (o,br) {						//dino init						var v:int = Math2.randInt(1,4);			switch (v) {				case 1:				case 2:					assignGFX(new _Stego);					break;				case 3:					assignGFX(new _Stego2);					break;				case 4:					assignGFX(new _Stego3);					break;			}									super.init (o,br,this);						//extra physics properties			ph.f = .9,			//friction			ph.bn = 0,			//bounce			ph.g = .5,			//gravity			ph.m = 250,			//mass			ph.r = 33			//radius						//other vars			var am:Number;			switch (brain.S.difNum) {								case 1: am=.4; break;				case 2: am=.5; break;				case 3: am=.67; break;				case 4: am=.82; break;							}			if (brain.S.df) {				am*=.5;			}						accelMax=accelMaxBase=(o.remote)?o.am:rand2(am-.1,am+.1);			accelF=accelFBase=.01;			angI=15;						//land area for raptor			hasExtra=true;			extraA = brain.create ( {nam:"StegoA", px:x, py:y+39, dad:this, r:66, depth:["world","main"]} )						//init dir			keys.R=true;			if (brain.S.df) {				keys.R=false;				keys.L=true;			}			vel.x=1;						//misc			xh=[0,0];			pData = { score:200 }						//broadcast creation?				trace ("stego",o.remote);			if (brain.S.mod=="MP"&&!o.remote&&!brain.S.df) { brain.sys.MP.sendToRoom ("OC"+"px:"+rnd(o.px)+";py:"+rnd(o.py)+";am:"+accelMax+";rdt:"+(o.rData?roomID:0)+";nam:Stego") }					}				// MAIN LOOP				public function main () {						//run functions common to all Dinos			mainDino ();						//rev?			xh.unshift (pos.x)			xh.pop();			if (Math.abs(xh[0]-xh[1])<.1) { rev() }						if (brain.S.df) {				keys.R=false;				keys.L=true;			}											}				public function removeExtras() {						extraA.deleteAll();			extraA=null;			hasExtra=false;					}				}	}