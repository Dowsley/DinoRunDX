package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Blood extends MovingDot {				private var c:uint=0;				public function Blood (o,brain):void {						//init			var colors:Array = [0xC70E0E,0xC40000,0xFF2222];			init (o, brain, false, colors[rand(0,colors.length-1)]);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .99,			//friction			ph.bn = 0,			//bounce			ph.g = .3,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						c++;			if (c>150) { deleteAll() }		}				public function remove (obj) {						deleteAll();					}			}	}