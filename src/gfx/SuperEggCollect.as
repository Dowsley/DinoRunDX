package gfx {		import flash.display.MovieClip;		import fx.ColorChange;		import supers.FX;		public class SuperEggCollect extends FX {				private var valCC:ColorChange;		//private var descCC:ColorChange;		private var ca:Array = [[255,255,255],[180,180,180],[0,255,255],[0,255,0],[0,0,255]];		private var a:Number;		private var d:int;				override public function nulls() {						super.nulls();			valCC.nulls();			valCC=null;			//descCC.nulls();			//descCC=null;			ca=null;					}				public var eggVal:MovieClip				public function SuperEggCollect (o,br):void {						assignGFX(new _SuperEggCollect);			eggVal = graphic.eggVal			init (o,br,this);						//egg value			eggVal.val.text="+"+o.val;			//eggDesc.val.text=o.desc;						valCC = new ColorChange (eggVal);			//descCC = new ColorChange (eggDesc);			d=0;			a=1.5;						}				public function main ():void {						//run functions common to all FX			mainFX ();						//flash and fade egg val			d++;			if (d==2) {								d=0;				a-=.04;				var cl=ca[rand(0,3)]				valCC.cChange ([cl[0],cl[1],cl[2],0,0,0,a]);				//descCC.cChange ([cl[0],cl[1],cl[2],0,0,0,a]);							}						eggVal.y-=1;			eggVal.x+=brain.dino.vel.x/rand2(1.1,1.2);			//eggDesc.y-=1;			//eggDesc.x+=brain.dino.vel.x/rand2(1.4,1.6);					}			}	}