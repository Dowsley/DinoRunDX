package gfx {		import flash.display.MovieClip;		import fx.ColorChange;		import supers.FX;		public class ScoreFloat extends FX {				private var valCC:ColorChange;		private var ca:Array = [[255,255,255],[180,180,180],[0,255,255],[0,255,0],[0,0,255]];		private var a:Number;		private var d:int;				override public function nulls() {						super.nulls();			valCC.nulls();			valCC=null;			ca=null;					}				private var vel:Number = 1;		private var velInc:Number = rand2(.01,.15);				public var scoreVal:MovieClip;				public function ScoreFloat (o,br):void {						assignGFX(new _ScoreFloat);			scoreVal = graphic.scoreVal;			init (o,br,this);						//bone value			scoreVal.val.text="+"+o.val;						valCC = new ColorChange (scoreVal);			d=0;			a=1.5;						}				public function main ():void {						//run functions common to all FX			mainFX ();						//flash and fade bone val			d++;			if (d==2) {								d=0;				a-=.04;				var cl=ca[rand(0,3)]				valCC.cChange ([cl[0],cl[1],cl[2],0,0,0,a]);							}			vel += velInc;			scoreVal.y-=vel;			scoreVal.x+=brain.dino.vel.x/rand2(1.1,1.2);								}			}	}