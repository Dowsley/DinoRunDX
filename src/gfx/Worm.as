package gfx {		import fx.ColorChange;		import supers.Clip;		public class Worm extends Clip {				public var pData:Object;		public var near:Boolean=true;		public var ug:Boolean;				override public function nulls() {						super.nulls();			pData=null;					}				public function Worm (o,br):void {						//init			assignGFX(new _Worm);			super.init (o,br,this);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = 20;			//radius			ph.g=0;						//pData			pData = { rFactor:1.2, rTime:250, jPow:1.15, jTime:300, score:50 }						//rev?			if (o.rev) { graphic.gotoAndPlay ("rev") }						//color variation			var cc = new ColorChange (this);			switch (brain.S.colorV) {								case "B":				cc.cChange ([-30,-40,-40,1,1,1,1]);				break;								case "C":				cc.cChange ([-30,-40,-40,1,1,1,1]);				break;							}						if (brain.S.gh) {				graphic.alpha = .25;			}									}				public function main ():void {			ug = graphic.ug;		}				public function eaten (obj) {						deleteAll();					}	}	}