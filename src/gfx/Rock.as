package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Rock extends MovingDot {				private var c:uint=0;				public function Rock (o,brain):void {						//init			if (o.colorV==undefined) {o.colorV=rand(1,3)}			var colors:Array = [0x666666,0x7B7B7B,0x968B8B];			init (o, brain, false, colors[o.colorV-1]);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .99,			//friction			ph.bn = .6,			//bounce			ph.g = .3,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						c++;			if (c>100) { deleteAll() }		}				// REACTIONS				public function bounce (v) {			vel.x=v.x;			vel.y=v.y;					}				public function remove (obj) {						deleteAll();					}			}	}