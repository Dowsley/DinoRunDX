package gfx {		import base.Brain;		import flash.display.MovieClip;
	public class Asteroid extends DinoClip {		public var brain:Brain;		public var o:Object;		public var NN:Boolean=false;	//near near var?		public var impact:Boolean;		public var ends:Boolean;		private var impacted:Boolean = false;		private var ended:Boolean = false;				public function Asteroid (o,br):void {						assignGFX(new _Asteroid);			this.o=o;			brain=br;			x=o.px;			y=o.py;					}				public function main ():void {						if (graphic.impact && !impacted) { impacted = true; brain.initImpact() }			if (graphic.ends && !ended) { ended = true; deleteAll() }					}				public function deleteAll () {						brain.deleteObj(this);					}				public function nulls () {						o=null;			brain=null;					}				public function finalActions() {}	}	}