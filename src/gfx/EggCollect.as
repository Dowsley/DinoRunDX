package gfx {		import flash.display.MovieClip;		import fx.ColorChange;		import supers.FX;		public class EggCollect extends FX {				private var valCC:ColorChange;		private var ca:Array = [[255,255,255],[180,180,180],[0,255,255],[0,255,0],[0,0,255]];		private var a:Number;		private var d:int;				override public function nulls() {						super.nulls();			valCC.nulls();			valCC=null;			ca=null;					}				public var eggVal:MovieClip;				public function EggCollect (o,br):void {						assignGFX(new _EggCollect);			eggVal = graphic.eggVal;			init (o,br,this);						//egg value			eggVal.val.text="+"+brain.sys2.stats.eggVal;						valCC = new ColorChange (eggVal);			d=0;			a=1.5;						}				public function main ():void {						//run functions common to all FX			mainFX ();						//flash and fade egg val			d++;			if (d==2) {								d=0;				a-=.04;				var cl=ca[rand(0,3)]				valCC.cChange ([cl[0],cl[1],cl[2],0,0,0,a]);							}			eggVal.y-=1.5;			eggVal.x+=brain.dino.vel.x/rand2(1.1,1.2);								}			}	}