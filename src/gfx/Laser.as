package gfx {		import supers.Line;	//	import flash.display.MovieClip;		public class Laser extends Line {				public var exData:Object;		private var c:uint=0;				public function Laser (o,brain):void {					init (o,brain);						//extra physics properties			ph.sh = "line";		//shape			ph.m = 5;			//mass						//exchange data			exData = {								id : "hit",				val: 30							}						}				public function main ():void {						//run functions common to all MovingClips			mainL ();						c++;			if (c>300) { remove(0) }					}				// REACTIONS				public function remove (p) {						deleteObj();			deleteClip();					}				public function depletePower (p) {											}			}	}