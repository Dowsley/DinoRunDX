package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Water extends MovingDot {				public var c:int=0;				public function Water (o,brain):void {						//init			var colors:Array;			if (o.white==undefined) {								colors = [0xAA1FADE9,0xCC1FADE9,0x991FADE9,0x771FADE9,0x33FFFFFF];							} else {								colors = [0xAA1FADE9,0x8856DDFE,0xAA56DDFE,0x66FFFFFF,0x33FFFFFF];							}							init (o, brain, true, colors[rand(0,colors.length-1)]);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .99,			//friction			ph.bn = 0,			//bounce			ph.g = .35,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						c++;			if (c>15) { deleteAll() }		}				// REACTIONS				public function remove (obj) {						deleteAll();					}			}	}