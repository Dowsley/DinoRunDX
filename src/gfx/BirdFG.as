package gfx {		import flash.display.MovieClip;	import flash.geom.Point;		import supers.MovingClip;		public class BirdFG extends MovingClip {				public var feeler:Object;		private var c:uint=0;		private var yAccel:Number=0;		private var xAccel:Number=0;		public var pData:Object;		private var dir:String;		private var oDir:String;		private var yA:Number=0;		public function BirdFG (o,brain):void {						//init			assignGFX(new _BirdFG);			init (o,brain,this);			near=true;						//gravity			ph.sh="circ";			ph.g = 0;					//dir			var dir:String = (rand(0,1)==0)?"L":"R";			graphic.gotoAndStop (dir);						//add feeler			hasExtra=true;			feeler = brain.create ( {nam:"Feeler", px:x, py:y, master:this, r:dir=="R"?rand(100,450):rand(300,450), depth:[0,0]} )						//pData			pData = { rFactor:1.25, rTime:400, jPow:1.25, jTime:1000, score:200 }							if (brain.S.gh) {				graphic.alpha = .25;			}					}				public function main ():void {						//run functions common to all MovingClips			mainMC ();						//face left or right			if (vel.x<-.1) { dir="L" }			if (vel.x>.1) { dir="R" }			if (oDir!=dir) { graphic.gotoAndStop (dir) }			oDir=dir;			if (c>0) {								c++;				switch (c) {										case 8:					yAccel=-.175;					feeler = brain.create ( {nam:"FeelerG", px:x, py:y, master:this, r:120, depth:[0,0]} );					hasExtra=true;					break;										case 18:					yAccel=-.05;					xAccel=.1;					break;									}								xAccel+=(brain.dino.pos.x>x)?rand2(-.03,.01):rand2(-.01,0.03);				yAccel+=rand2(-.02,.02);				yA=0;							}						//adjust velocity			vel.offset (xAccel,yAccel-yA);						//remove?			if (Math.abs(x-brain.dino.pos.x)>2500) {								deleteAll();							}		}				// REACTIONS				public function dinoClose () {}		public function feelerContact (obj) {									if (obj==brain.dino) {								feeler.deleteAll();				feeler=null;				hasExtra=false;								//launch!				near=false;				graphic.gotoAndStop ("R");				body.gotoAndPlay ("launch");				c=1;							}					}				public function feelerGContact () {						yA=xAccel/2;					}				public function eaten (obj) {						//flyin feathers!			var vv=getVV(obj.vel)			var l=rand2(6,10);			for (var i:uint=1; i<l; i++) { brain.create ({nam:"Feather", px:x+rand(-9,9), py:y+rand(-9,9), vx:rand2(vv*-1,vv), vy:rand2(vv*-1,vv*2), depth:["world","main"]}) }			deleteAll();					}				public function removeExtras () {						feeler.deleteAll();			feeler=null;			hasExtra=false;			pData=null;					}			}	}