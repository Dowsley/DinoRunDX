package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Leaf extends MovingDot {				public function Leaf (o,brain):void {						//init			var colors:Array = [0x3E9933,0x289328,0x30A546];			init (o, brain, false, colors[rand(0,colors.length-1)]);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .99,			//friction			ph.bn = 0,			//bounce			ph.g = .04,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						//fall like a leaf			vel.x*=.98;			vel.y*=.98;			if (vel.y<.1) { vel.y=.1 }			vel.x+=rand2(-1,1);			vel.y+=rand2(-.1,.2);		}				// REACTIONS				public function remove (obj) {						deleteAll();					}			}	}