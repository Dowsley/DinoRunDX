package gfx {		import flash.display.MovieClip;		import fx.ColorFlash;		import supers.Clip;		public class Pup extends Clip {				public var pData:Object;		private var col:String;				override public function nulls() {						super.nulls();			pData=null;					}				public function Pup (o,brain):void {					assignGFX(new _Pup);			init (o,brain,this);						//extra physics properties			ph.sh = "circ";						//exchange data			pData = {								id : o.typ,				val: o.lvl							}						//exceptions			if (o.typ=="strength") { ph.r=12 }						//appearance			graphic.gotoAndStop (o.typ+o.lvl);			switch (pData.id) {								case "speedBoost":col="Y";break;				case "superJump":col="P";break;				case "strength":col="R";break;							}		}				public function main ():void {						//functions for all Clips			mainC();						//sparks			brain.create ( {nam:"DotFX", colr:col, alph:1, c:20, velA:.925, px:x, py:y, vx:rand(-7,7), vy:rand(-7,7), front:true, depth:["world","bgx"]} );			brain.create ( {nam:"DotFX", colr:col, alph:1, c:20, velA:.925, px:x, py:y, vx:rand(-7,7), vy:rand(-7,7), front:true, depth:["world","bgx"]} );					}				// REACTIONS				// collected		public function collected (obj) {						if (!obj.o.remote) {							var dirA = [[0,-1],[.65,-.65],[1,0],[.65,.65],[0,1],[-.65,.65],[-1,0],[-.65,-.65],[0,-.5],[.33,-.33],[.5,0],[.33,.33],[0,.5],[-.33,.33],[-.5,0],[-.33,-.33]];								switch (pData.id) {										case "speedBoost":					//speedboosts can only be eaten if he dino is ducking					if (brain.dino.keys.D) {												for (var i:int=0; i<16; i++) { brain.create ( {nam:"DotFX", colr:"Y", alph:1.5, c:60, velA:.99, px:x, py:y, vx:dirA[i][0], vy:dirA[i][1], depth:["world","bgx"]} ) }						brain.sys.audio.ply ("speedBoost"+o.lvl,.9,false)						deleteAll();											}					break;										case "superJump":					for (var ii:int=0; ii<16; ii++) { brain.create ( {nam:"DotFX", colr:"P", alph:1.5, c:60, velA:.99, px:x, py:y, vx:dirA[ii][0], vy:dirA[ii][1], depth:["world","bgx"]} ) }					brain.sys.audio.ply ("superJump",.9,false)					deleteAll();					break;										case "strength":					for (var j:int=0; j<16; j++) { brain.create ( {nam:"DotFX", colr:"R", alph:1, c:60, velA:.98, px:x, py:y, vx:dirA[j][0], vy:dirA[j][1], depth:["world","bgx"]} ) }					for (var k:int=0; k<16; k++) { brain.create ( {nam:"DotFX", colr:"R", alph:1.5, c:90, velA:.99, px:x, py:y, vx:dirA[k][0]*1.5, vy:dirA[k][1]*1.5, depth:["world","bgx"]} ) }					brain.sys.audio.ply ("strength",.7,false)					deleteAll();					break;									}							} else { try { gotoAndPlay (o.typ+o.lvl+"Grow") } catch (e) {} }					}			}	}