package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Feather extends MovingDot {				public function Feather (o,brain):void {						//init			init (o, brain, false, 0xFFFFFF);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .99,			//friction			ph.bn = 0,			//bounce			ph.g = .04,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						//fall like a feather			vel.x*=.96;			vel.y*=.96;			if (vel.y<.1) { vel.y=.1 }			vel.x+=rand2(-1,1);			vel.y+=rand2(-.1,.2);		}				// REACTIONS				public function remove (obj) {						deleteAll();					}			}	}