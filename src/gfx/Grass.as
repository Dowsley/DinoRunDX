package gfx {		import supers.MovingDot;	//	import flash.display.Bitmap;	import flash.display.BitmapData;		public class Grass extends MovingDot {				private var c:uint=0;		private var colorV:Object = { A:0x33CC33, B:0x33CC33, C:0x976824, V:0x141414 }				override public function nulls() {						super.nulls();			colorV=null;					}				public function Grass (o,brain):void {						//init			init (o, brain, false, o.ug?0x754C24:colorV[brain.S.colorV]);						//extra physics properties			ph.sh = "circ"		//shape			ph.f = .2,			//friction			ph.bn = 0,			//bounce			ph.g = .2,			//gravity			ph.m = 1			//mass						}				public function main ():void {						//run functions common to all MovingDots			mainMD ();						c++;			if (c>50) { deleteAll() }		}				// REACTIONS				public function bounce (v) {			vel.x=v.x;			vel.y=v.y;					}				public function remove (obj) {						deleteAll();					}			}	}