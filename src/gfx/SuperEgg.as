package gfx {		import flash.display.MovieClip;		import fx.ColorFlash;		import supers.Clip;		import util.Stats;
		public class SuperEgg extends Clip {				private var scores:Object;				override public function nulls() {						super.nulls();			scores=null;					}				public function SuperEgg (o,brain):void {					assignGFX(new _SuperEgg);			init (o,brain,this);						//extra physics properties			ph.sh = "circ";			ph.r=(o.typ!="A")?width/2:24;						//appearance			graphic.gotoAndStop (o.typ);						//scores + decription			scores={ ANum:5000, ATxt:"Alien Egg", 					 BNum:25000, BTxt:"Bronto Egg",					 TNum:3000, TTxt:"T-Rex Egg",					 MNum:10000, MTxt:"Magno Egg",					 SNum:2500, STxt:"Spino Egg",					 DNum:7500, DTxt:"Diplodo Egg",					 GNum:4000, GTxt:"Gigano Egg",					 ONum:5000, OTxt:"Oloro Egg",					 INum:6000, ITxt:"Iguanodon Egg",					 RNum:7500, RTxt:"Rinchenia Egg" }						}				public function main ():void {						//functions for all Clips			mainC();					}				// REACTIONS				// collected		public function collected (obj) {						brain.sys.audio.ply ("superEgg",.6,false);			var stats:Stats = brain.sys2.stats;			if (o.typ!="B") { stats.addSuperEgg(o.typ,scores[o.typ+"Num"],scores[o.typ+"Txt"]) } else if (!brain.S.cheat) {								stats.superEggsPL.push ("B");				stats.addPLScore (scores[o.typ+"Num"]);				stats.DNA+=stats.superEggPTS.B[0];				stats.GP+=stats.superEggPTS.B[1];				stats.superEggsPLNum=stats.superEggsPL.length;				stats.checkMS();				stats.saveGame();								//milestone				stats.lvlMS.push(new Array("Bronto Egg Saved","+25000 Player Score, +100 DNA, +2000 Bones",[]));								}							brain.create ( {nam:"SuperEggCollect", val:((brain.S.mod=="MP")?2:scores[o.typ+"Num"]), px:x, py:y, depth:["world","main"]} )			deleteAll();					}			}	}