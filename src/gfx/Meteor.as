package gfx {		import flash.display.MovieClip;		public class Meteor extends DinoClip {				public var xm:Number;		public var ym:Number;		public var brain:Object;		public var o:Object;		public var NN:Boolean=false;	//near near var?				public function Meteor (o,br):void {						assignGFX(new _Meteor);			this.o=o;			brain=br;			x=o.px;			y=o.py;			xm=(o.d=="bg")?rand(6,9):rand(9,12);			ym=xm*1.3333;			graphic.gotoAndStop ("m"+((o.d=="bg")?rand(1,5):rand(6,10)));			alpha=(o.d=="bg")?.5:.8;					}			public function main ():void {						x+=xm;			y+=ym;			if (y>600) { 							brain.weather.lStrike(rand(2,3),(o.d=="bg")?.3:.6);				deleteAll();				brain.world.shakeC=(o.d=="bg")?20:40;							}		}				public function deleteAll () {						brain.deleteObj(this);					}				public function nulls () {						brain=null;			o=null;					}				public function finalActions() {}				public function rand(min:int, max:int):int {			return (Math.round(Math.random()*(max-min))+min);		}			}	}