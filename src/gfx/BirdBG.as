package gfx {		import flash.display.MovieClip;		import fx.ColorChange;		public class BirdBG extends DinoClip {				public var xm:Number;		public var ym:Number;		public var txm:Number;		public var tym:Number;		public var brain:Object;		public var o:Object;		private var c:uint=0;		public var NN:Boolean=false;	//near near var?		private var colors=[[],[131,168,192],[131,168,192],[109,109,180],[209,142,114],[131,168,192],[0,42,81],[123,77,47],[66,43,77],[206,153,111],[222,118,106],[0,11,48],[0,11,48],[23,0,32],[34,0,0],[0,11,48],[0,34,99],[62,16,0],[34,4,53],[67,27,16],[141,178,215],[132,13,0],[197,139,101],[228,120,142],[208,30,30],[231,163,118],[253,101,36],[227,128,28],[226,117,71],[],[222,118,106],[],[],[],[],[],[],[],[],[],[132,13,0]]				public function BirdBG (o,br):void {						assignGFX(new _BirdBG);			this.o=o;			brain=br;			x=o.px;			y=o.py;			xm=txm=4;			ym=tym=0;						//colorV			var cc = new ColorChange (this);			if (brain.S.gh) {				cc.cChange ([255,255,255,1,1,1,.11]);			} else {				var ca:Array = colors[brain.S.sky];				cc.cChange ([ca[0],ca[1],ca[2],0,0,0,1]);				cc.nulls();			}					}				public function main ():void {						//fly			txm+=rand(-.03,.03);			tym+=rand(-.1,.1);						xm+=(txm-xm)/10;			ym+=(tym-ym)/2;						x+=xm;			y+=ym;						c++;			if (c>1000) { deleteAll() }		}				public function deleteAll () {						brain.deleteObj(this);			//brain.deleteClip(this);					}				public function nulls() {						for (var oo in o) { delete o[oo] }			o=null;			brain=null;			colors=null;					}				public function finalActions() {}				private function rand(min:Number, max:Number):Number {			return (Math.random()*(max-min))+min;		}			}	}