package util {		import supers.Circle;		public class Feeler extends Circle {				private var master:Object;				public function Feeler (o,br):void {						//init			super.init (o,br);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = o.r;			//radius			big=true;						//master			master=o.master;						}				public function main ():void {						//follow master			pos.x=master.pos.x;			pos.y=master.pos.y;					}				override public function nulls () {						super.nulls();			master=null;					}								// REACTIONS		public function contact (o) {						 master.feelerContact(o);		}			}	}