package util {		import supers.Circle;		public class RevArea extends Circle {				private var c:uint=0;				public function RevArea (o,br):void {						//init			super.init (o,br);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = 40;			//radius						}				public function main ():void {						if (c>0) { c-- }					}				public function contact (obj) {						if (c==0) {								obj.rev();				c=30;							}					}											}	}