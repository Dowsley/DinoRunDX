package util {		import supers.Circle;	import flash.geom.Point;		public class FeelerF extends Circle {				private var master:Object;				public function FeelerF (o,br):void {						//init			super.init (o,br);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = o.r;			//radius			big=true;						//master			master=o.master;						}				public function main ():void {						//follow master			pos.x=master.pos.x;			pos.y=master.pos.y;					}				override public function nulls () {						super.nulls();			master=null;					}		// REACTIONS		public function contact (o) {						 master.foodClose();		}			}	}