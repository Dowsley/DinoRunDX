package util {		import flash.display.MovieClip;		public class EventTimer {				private var c:uint;		private var endF:String;		private var clip:MovieClip;				public function EventTimer (clip:MovieClip,cNum:uint,endF:String):void {						this.clip=clip;			this.c=cNum;			this.endF=endF;		}				public function main () {						if (c--<1) {								clip[endF]();				clip.removeSYS ("eventTimer");							}					}			}	}