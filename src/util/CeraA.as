package util {		import flash.geom.Point;		import supers.MovingClip;		public class CeraA extends MovingClip {				public var dad:Object;				public function CeraA (o,br):void {						//init			super.init (o,br,this);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = o.r;			//radius			ph.m = 200;			ph.f = .9,			//friction			ph.bn = 0,			//bounce						//misc			ride=true;			dad=o.dad;			big=true;			o.typ="Dino";						}				override public function ghostTouched():void		{			//trace (999);			gt = true;		}				override public function nulls () {						super.nulls();			dad=null;					}				public function main ():void {						//center is always 39 down, perp to cera center			pos = dad.pos.add (Point.polar(42, dad.ang+(Math.PI/2)));						//vel			vel.x=dad.vel.x;			vel.y=dad.vel.y;						dad.RR=RR;						if (gt) {				dad.alpha-=.005;				if (alpha < 0) {					deleteAll();				}			}								}				public function bounce (v) {						dad.bounce (v);					}				public function crush (obj) {						dad.crush(obj);					}				override public function applyVel (v:Point) {						dad.applyVel (v);					}			}	}