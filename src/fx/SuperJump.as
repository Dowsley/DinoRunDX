package fx {		import flash.display.MovieClip;		public class SuperJump {				private var clip:MovieClip;		private var cMax:uint;		private var c:int=0;		private var jumpDec:Number;						public function SuperJump (clip:MovieClip,pow,cMax):void {						this.clip=clip;			this.cMax=cMax;			clip.jumpPow=clip.jumpPowOrig*pow;			jumpDec=(pow-clip.jumpPowOrig)/(cMax/2);					}				public function main () {			c++;						//sparkles			if (!clip.near) { if (Math.random()<(clip.jumpPow-1)) {								var dirA=(Math.random()<.5)?[[0,-1],[1,0],[0,1],[-1,0]]:[[.65,-.65],[.65,.65],[-.65,.65],[-.65,-.65]];				for (var i:int=0; i<4; i++) { 									clip.brain.create ( {nam:"DotFX", colr:"P", alph:.75, c:30, velA:.95, px:clip.x, py:clip.y+(clip.ph.r/2), vx:dirA[i][0], vy:dirA[i][1], depth:["world","main"]} ) ;									}							} }						if (c>(cMax/2)) { clip.jumpPow=clip.jumpPowOrig+jumpDec*(cMax-c) }			if (c>=cMax) { remove() }					}				public function addOn (pow,time) {						if (pow>clip.jumpPow) { clip.jumpPow = pow}			cMax+=time*.75;			jumpDec=(pow-clip.jumpPowOrig)/(cMax/2);					}						public function remove () {						clip.jumpPow=clip.jumpPowOrig;			clip.removeSYS ("superJump");			clip=null;						}			}	}