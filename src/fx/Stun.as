package fx {		import flash.display.MovieClip;		public class Stun {				private var dino:MovieClip;		private var c:uint=0;				public function Stun (dino:MovieClip):void {						this.dino=dino;			//dino.app=dino.baseApp="skid";			//dino.master.body.head.gotoAndPlay ("bonk");			dino.vel.y-=dino.rand2(5,7);			dino.vel.x-=dino.rand2(8,10);			dino.pos.y-=3;		}				public function main () {						c++;			if (c>20) {								remove();							} else {								dino.accelMax=.3;							}					}				public function remove () {						dino.app=dino.baseApp="";			if (dino.accelMaxBase<.9) { dino.accelMaxBase=.9 }			dino.accelMax=dino.accelMaxBase;			dino.velXMax=14*dino.accelMax;			dino.removeSYS ("stun") ;			dino=null;					}			}	}