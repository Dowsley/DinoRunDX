package fx {		import supers.Circle;		public class Gen extends Circle {				public function Gen (o,br):void {						//init			super.init (o,br);						}				public function main ():void {						if (rand(0,100)==0) {								switch (o.lvl) {										case "A":					//runner					brain.create ({nam:"Runner", px:o.px, py:o.py, vx:0, vy:0, free:true, rd:true, depth:["world","main"]});					break;										case "B":					//lizard					var ca:Array = ["G","O","P"];					brain.create ({nam:"Liz"+ca[rand(0,2)], px:o.px, py:o.py, vx:0, vy:0, free:true, rd:true, depth:["world","main"]});					break;									}							}					}			}	}