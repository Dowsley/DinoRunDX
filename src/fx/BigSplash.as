package fx {		import supers.Circle;		public class BigSplash extends Circle {				public function BigSplash (o,br):void {						//init			super.init (o,br);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = 25;			//radius						}				public function main ():void {					}				public function hit (obj:Object) {						var l=rand2(2,4);			for (var i:uint=1; i<l; i++) { brain.create ({nam:"Water", white:true, px:pos.x+rand2(-20,20), py:pos.y, vx:rand2(-6,6), vy:rand(-6,0), depth:["world","main"]}) }					}											}	}