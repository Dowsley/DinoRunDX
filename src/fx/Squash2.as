package fx {		import flash.display.MovieClip;		public class Squash2 {				private var clip:MovieClip;		private var c:uint=0;				public function Squash2 (clip:MovieClip):void {						this.clip=clip;			//clip.ph.r-=9;			clip.app=clip.baseApp="squaB";			clip.brain.sys.audio.ply ("squash"+clip.rand(1,2),.8,false);		}				public function main () {						c++;						//remove?			if (c>=16) {								remove()											} else {								clip.accelMax=.3;							}					}				public function remove () {						if (clip.accelMaxBase<.9) { clip.accelMaxBase=.9 }			clip.accelMax=clip.accelMaxBase;			clip.velXMax=14*clip.accelMax;			clip.app=clip.baseApp="";			clip.removeSYS ("squash2");			clip=null;					}			}	}