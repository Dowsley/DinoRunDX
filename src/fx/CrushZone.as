package fx {		import supers.Circle;	import flash.geom.Point;		public class CrushZone extends Circle {				public var c:int=0;		public var vel:Point;				public function CrushZone (o,br):void {						//init			super.init (o,br);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = o.r;			//radius			vel = new Point (o.r/15,o.r/15);									}				override public function nulls () {						super.nulls();			vel=null;					}				public function main ():void {						c++;			if (c==2) { deleteAll() }					}	}	}