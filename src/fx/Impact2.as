package fx {		import flash.display.MovieClip;		public class Impact2 {				var brain:Object;		var fgc:int=0;		var fga:Number=0;		var fgi:Number=50;				public function Impact2 (brain:Object):void {						this.brain=brain;					}				public function main () {						//FG			fgc++;			fga+=fgi;			var fgx:Number=fga/250;			brain.world.bgCC.cChange ([fga,fga,fga/1.5,fgx,fgx,fgx,1]);			brain.world.mainCC.cChange ([fga,fga,fga/1.5,fgx,fgx,fgx,1]);			brain.world.fgCC.cChange ([fga,fga,fga/1.5,fgx,fgx,fgx,1]);			switch (fgc) {								case 5: fgi=-3; break;				case 90: fgi=fga=0; break;				case 95: remove(); break;							}								}				public function restart () {						var fgc:int=2;			var fga:Number=100;			var fgi:Number=50;					}							public function remove () {						brain.removeSYS2("impact2"); 			brain=null;					}			}	}