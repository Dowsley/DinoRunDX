package fx {		import flash.display.MovieClip;		public class NoRide {				private var dino:MovieClip;		private var c:uint=0;				public function NoRide (dino:MovieClip):void {						this.dino=dino;		}				public function main () {						c++;			if (c>25) { remove() } 					}				public function remove () {						dino.removeSYS ("noRide") ;			dino=null;					}			}	}