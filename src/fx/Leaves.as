package fx {		import supers.Circle;		public class Leaves extends Circle {				public function Leaves (o,br):void {						//init			super.init (o,br);						//extra physics properties			ph.sh = "circ";		//shape			ph.r = o.lvl*10;	//radius						}				public function main ():void {					}				public function hitLeaves (obj:Object) {						var dinoPos=obj.pos;			var vel=obj.vel;			var vv=Math.sqrt((vel.x*vel.x)+(vel.y*vel.y));			var l=rand2(-5,vv-4);			for (var i:uint=1; i<l; i++) { brain.create ({nam:"Leaf", px:dinoPos.x+rand(-15,15), py:dinoPos.y+rand(-15,15), vx:rand2(vv*-.5,vv), vy:rand2(vv*-1,vv), depth:["world","main"]}) }					}											}	}