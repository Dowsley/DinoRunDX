package fx {		import flash.display.MovieClip;	import fx.DinoGlow;		public class Adren {				private var clip:MovieClip;		private var c:int;		private var vals:Array = [[0,0],[.3,200,.15,300],[.35,250,.2,250],[.25,250,.2,300],[.15,250,.25,350]]				public function Adren (clip:MovieClip):void {						this.clip=clip;						//boost			var d:int=clip.brain.S.difNum;			clip.speedBoost (clip.accelMaxBase+(vals[d][0]*clip.adP),vals[d][1]);			clip.superJump (clip.jumpPowOrig+(vals[d][2]*clip.adP),vals[d][3]);			c=500;						//reduce next adranline rush			clip.adP*=.5;					}				public function main () {						c--;			if (c<0) { remove() }					}				public function remove () {						clip.removeSYS2 ("adren"); 			clip=null;			vals=null;					}			}	}